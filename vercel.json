{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "node_modules/swagger-ui-dist/**",
          "swagger-output.json"
        ]
      }
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/server.js" }]
}
